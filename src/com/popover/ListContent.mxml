<?xml version="1.0" encoding="utf-8"?>
<s:List xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark"
		width="250">
	<s:layout>
		<s:VerticalLayout gap="0"
						  horizontalAlign="contentJustify"
						  requestedRowCount="-1"/>
	</s:layout>
	<s:itemRenderer>
		<fx:Component>
			<s:IconItemRenderer iconField="icon" labelField="name" 
								iconHeight="32" iconWidth="32"
								iconPlaceholder="@Embed(source='data/ru.png')"/>
		</fx:Component>
	</s:itemRenderer>
	<s:ArrayCollection>
		<fx:Object name="Russia" icon="data/ru.png"/>
		<fx:Object name="Denmark" icon="data/dm.png"/>
		<fx:Object name="Great Britain" icon="data/uk.png"/>
	</s:ArrayCollection>
</s:List>
